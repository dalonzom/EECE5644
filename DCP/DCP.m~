load('iris.mat') 
% Use 80% for training 
train = randperm(150); 
test = iris(train(121:150),:)
iris = iris(train(1:120),:); 
numAttributes = 4; 
numClasses = 3; 

%% Create Dominance Classifier Structure 
training_set = {}; 
for i = 1:numAttributes 
    training_set{i} = training(iris(:,i), iris(:,5)); 
end 

%% Classification 

% Guess Classes 
guess = {}; 
for i = 1:size(test,1)
    point = test(i,:); 
    guess{i} =  guessClasses(point, numAttributes, training_set); 
end 

%% Combine Dominances 

% Voting Method 1 
results = vm1(test, guess, numAttributes);  

% Voting Method 2 
one = 0; 
two = 0; 
three = 0; 

for i = 1:numAttr
one = one + training_set






